<context>
  <signature>
    <name>Datastorage</name>
  </signature>
  <adapter>
    <name>MySQL</name>
  </adapter>
  <!--
    datastorage bot required operations
    insert
    update
    delete
    fetch
   -->
  <operations>
		<start>
			<path>org.abn.bot.db.operation.Start</path>
		</start>
		<kill>
			<path>org.abn.bot.db.operation.Stop</path>
		</kill>
    <statusReport>
      <path>org.abn.bot.db.operation.StatusReport</path>
    </statusReport>
    <insert>
      <path>org.abn.bot.db.operation.Insert</path>
    </insert>
  </operations>
	<xmpp>
		<username>bot.datastorage.abn_botdb</username>
		<password>admin</password>
		<server>localhost.com</server>
	</xmpp>
</context>
